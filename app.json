{
  "expo": {
    "name": "NollyFlix",
    "description": "Buy & Rent Nollywood Movies",
    "scheme": "nollyflix",
    "slug": "nollyflix",
    "privacy": "public",
    "version": "0.0.2",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "orientation": "portrait",
    "primaryColor": "#000000",
    "icon": "src/assets/nollyflix2.png",
    "splash": {
      "backgroundColor": "#000000",
      "image": "src/assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.nollyflix.tv",
      "versionCode": 2,
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    },
    "ios": {
      "buildNumber": "0.0.2",
      "supportsTablet": true
    }
  }
}
